mkdir TEMP
INPUT_STRING=hello
while [ "$INPUT_STRING" != "bye" ]
do
  wget http://www.skeelerteamevergem.be/Live_results/VK2019/CSV/MM.csv -O ./TEMP
  mv ./TEMP/MM.csv MM.csv
  sleep 2
  wget http://www.skeelerteamevergem.be/Live_results/VK2019/CSV/MJ.csv
  sleep 2
  wget http://www.skeelerteamevergem.be/Live_results/VK2019/CSV/PM.csv
  sleep 2
  wget http://www.skeelerteamevergem.be/Live_results/VK2019/CSV/PJ.csv
  sleep 2
  wget http://www.skeelerteamevergem.be/Live_results/VK2019/CSV/SM.csv
  sleep 2
  wget http://www.skeelerteamevergem.be/Live_results/VK2019/CSV/SJ.csv
  sleep 2
  wget http://www.skeelerteamevergem.be/Live_results/VK2019/CSV/CM.csv
  sleep 2
  wget http://www.skeelerteamevergem.be/Live_results/VK2019/CSV/CJ.csv
  sleep 2
  wget http://www.skeelerteamevergem.be/Live_results/VK2019/CSV/JD.csv
  sleep 2
  wget http://www.skeelerteamevergem.be/Live_results/VK2019/CSV/JH.csv
